{
  "name": "appifi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node build/app.js",
    "build": "babel src --out-dir build",
    "devel": "nodemon --exec babel-node --ignore web src/app.js",
    "devel-bootstrap": "nodemon --exec babel-node --ignore web src/app.js --fake-bootstrap",
    "devel-assets": "nodemon --exec babel-node --ignore web --ignore public/bundle.js assets.js",
    "devel-appstore": "nodemon --exec babel-node --ignore web --ignore public/bundle.js src/app.js --appstore-master",
    "webpack": "webpack --watch --watch-poll",
    "unit": "node_modules/.bin/mocha --compilers js:babel-core/register --recursive test",
    "fruit-devel": "PORT=3721 nodemon --exec babel-node --ignore tmptest src/fruitmix/main.js --path tmptest",
    "fruit-family": "PORT=3721 nodemon --ignore 'family/*' --exec babel-node src/fruitmix/main.js --family",
    "fruit-unit": "NODE_PATH=. mocha --compilers js:babel-core/register --recursive --sort test/fruitmix/unit",
    "fruit-algo": "NODE_PATH=. NODE_ENV=test mocha --compilers js:babel-core/register --recursive --sort test/fruitmix/algo",
    "fruit-agent": "NODE_PATH=. NODE_ENV=test mocha --compilers js:babel-core/register --recursive --sort test/fruitmix/agent",
    "mochamon": "NODE_ENV=test nodemon --exec mocha --compilers js:babel-core/register --recursive --sort",
    "mocha": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive --sort",
    "lyc": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --compilers js:babel-core/register",
    "lycmon": "NODE_ENV=test nodemon --exec nyc --reporter=lcov --reporter=text mocha --compilers js:babel-core/register"
  },
  "dependencies": {
    "babel-runtime": "https://registry.npmjs.org/babel-runtime/-/babel-runtime-6.18.0.tgz",
    "bcrypt": "^0.8.7",
    "bcryptjs": "^2.3.0",
    "binary-loader": "0.0.1",
    "bluebird": "^3.4.1",
    "body-parser": "1.13.3",
    "canonical-json": "0.0.4",
    "clone": "^1.0.2",
    "cookie-parser": "~1.3.5",
    "crypt3": "^1.0.0",
    "cuint": "^0.2.2",
    "dataformat": "^1.0.0",
    "debug": "~2.2.0",
    "deep-equal": "^1.0.1",
    "deep-freeze": "0.0.1",
    "deepmerge": "^0.2.10",
    "express": "~4.13.1",
    "filter-object": "^2.1.0",
    "fs-xattr": "^0.1.14",
    "json-loader": "^0.5.4",
    "jwt-simple": "^0.5.0",
    "mkdirp": "^0.5.1",
    "morgan": "~1.6.1",
    "nodeify": "^1.0.1",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-jwt": "^2.1.0",
    "pidusage": "^1.1.1",
    "redux": "^3.3.1",
    "respawn": "^2.4.1",
    "rimraf": "^2.5.3",
    "sanitize-filename": "^1.6.0",
    "serve-favicon": "~2.3.0",
    "shallowequal": "^0.2.2",
    "superagent": "^1.8.3",
    "upnpserver": "^2.1.3",
    "ursa": "^0.9.4",
    "validator": "^5.3.0",
    "xxhash": "^0.2.4",
    "xxhashjs": "^0.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.1.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-bluebird": "^1.0.0",
    "babel-plugin-transform-es2015-template-literals": "^6.8.0",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-bluebird": "^1.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.17.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.9.0",
    "istanbul": "^0.4.5",
    "material-ui": "^0.17.0",
    "mocha": "^2.5.3",
    "node-loader": "^0.6.0",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.9.2",
    "nyc": "^10.1.2",
    "prettysize": "0.0.3",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-tap-event-plugin": "^2.0.0",
    "sinon": "^1.17.4",
    "supertest": "^2.0.0",
    "webpack": "^1.12.14"
  }
}
